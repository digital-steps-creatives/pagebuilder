{
    "name": "dscreatives/pagebuilder",
    "description": "drag and drop page builder",
    "keywords": ["Laravel Page builder"],
    "require": {
        "php": ">=7",
        "hansschouten/phpagebuilder": "^0.11"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Steps Creatives",
            "email": "development@dsc.co.ke"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dsc\\LaravelPageBuilder\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Dsc\\LaravelPageBuilder\\ServiceProvider"
            ],
            "aliases": {
                "PageBuilder": "Dsc\\LaravelPageBuilder\\Facade"
            }
        }
    }
}
